<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/tree/description/1.0.0" name="treeview" version="8.1.0">
  <ownedViewpoints name="ThemeTreeView">
    <ownedRepresentations xsi:type="description_1:TreeDescription" name="model" initialisation="true" showOnStartup="true" domainClass="theme.Model">
      <subItemMappings name="page" domainClass="theme.Page" semanticCandidatesExpression="feature:eAllContents">
        <defaultStyle labelExpression="feature:title">
          <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
        </defaultStyle>
        <subItemMappings name="pagePart" domainClass="theme.PagePart" semanticCandidatesExpression="feature:eAllContents">
          <defaultStyle>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
          </defaultStyle>
          <subItemMappings name="section" domainClass="theme.Section" semanticCandidatesExpression="feature:eAllContents">
            <defaultStyle labelExpression="Section">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='white']"/>
            </defaultStyle>
          </subItemMappings>
        </subItemMappings>
      </subItemMappings>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
